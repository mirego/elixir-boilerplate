    "copy-webpack-plugin": "^4.5.0",
    "css-loader": "^0.28.10",
    "eslint": "4.19.1",
    "eslint-plugin-mirego": "0.0.1",
    "mini-css-extract-plugin": "^0.4.0",
    "optimize-css-assets-webpack-plugin": "^4.0.0",
    "prettier": "1.16.4",
    "semver": "6.0.0",
    "stylelint": "9.2.1",
    "stylelint-config-mirego": "1.0.1",
    "stylelint-order": "0.8.1",
    "svgo": "1.0.5",
    "uglifyjs-webpack-plugin": "^1.2.4",
    "webpack": "4.4.0",
    "webpack-cli": "^2.0.10"
