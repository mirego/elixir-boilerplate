{
  "name": "elixir-boilerplate",
  "version": "0.0.1",
  "private": true,
  "engine-strict": true,
  "engines": {
    "node": "^12.15",
    "npm": "^6.13"
  },
  "scripts": {
    "deploy": "npm run enforce-engine-versions && webpack --mode production",
    "watch": "npm run enforce-engine-versions && webpack --mode development --colors --watch-stdin",
    "enforce-engine-versions": "./scripts/enforce-engine-versions.js",
    "preinstall": "npm run enforce-engine-versions",
    "postinstall": "npm run enforce-engine-versions"
  },
  "dependencies": {
    "babel-polyfill": "^6.26.0",
    "phoenix": "phoenixframework/phoenix#v1.5.5",
    "phoenix_html": "phoenixframework/phoenix_html#v2.14.2",
    "simple-css-reset": "^3.0.0"
  },
  "devDependencies": {
    "@babel/core": "^7.0.0",
    "@babel/preset-env": "^7.0.0",
    "babel-eslint": "^10.0.0",
    "babel-loader": "^8.0.0",
    "copy-webpack-plugin": "^4.5.0",
    "css-loader": "^2.1.1",
    "eslint": "^6.8.0",
    "eslint-plugin-mirego": "0.0.1",
    "mini-css-extract-plugin": "^0.4.0",
    "optimize-css-assets-webpack-plugin": "^4.0.0",
    "prettier": "1.16.4",
    "semver": "6.0.0",
    "stylelint": "9.2.1",
    "stylelint-config-mirego": "1.0.1",
    "stylelint-order": "0.8.1",
    "svgo": "1.0.5",
    "uglifyjs-webpack-plugin": "^1.2.4",
    "webpack": "4.4.0",
    "webpack-cli": "^2.0.10"
  }
}
