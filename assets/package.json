{
  "name": "elixir-boilerplate",
  "version": "0.0.1",
  "private": true,
  "engine-strict": true,
  "engines": {
    "node": "~10.14",
    "npm": "~6.4"
  },
  "scripts": {
    "deploy": "npm run enforce-engine-versions && npx webpack --mode production",
